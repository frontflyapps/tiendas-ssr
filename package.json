    "<<<<<<<<<<<<<<<VeoVeo>>>>>>>>>>>>>>>": "",
    "start:VeoVeo": "NG_APP_NAME=VeoVeo ng serve VeoVeo --host 0.0.0.0 --public-host tienda.tiendalocal.com --port 4308 --disable-host-check true",
    "build:VeoVeo": " ng build VeoVeo",
    "start:ssr:VeoVeo": "NG_APP_NAME=VeoVeo ng run VeoVeo:serve-ssr --port 4308",
