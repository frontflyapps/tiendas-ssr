    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start:ssr": "ng run tienda-v2:serve-ssr",
    "serve:ssr": "node dist/tienda-v2/server/main.js",
    "build:ssr": "ng build && ng run tienda-v2:server",
    "prerender": "ng run tienda-v2:prerender",
    "prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx,html}'",
